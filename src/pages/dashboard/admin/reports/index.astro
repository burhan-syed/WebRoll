---
import { getUser } from "@astro-auth/core";
import QueryPage from "../../../../components/admin/Query/QueryPage";
import MainLayout from "../../../../layouts/MainLayout.astro";
import type { User } from "../../../../types";
const user = getUser({ client: Astro }) as User;
if (!user || user.role !== "ADMIN") {
  return redirectUser("/signup");
}
---

<MainLayout SEOProps={{title: "Reports Query | Webroll", noindex: true, nofollow:true}}  noProse={true} noScrollBar={true}>
  <QueryPage mode={"REPORTS"} client:only="react" />
</MainLayout>
